{
   "name": "project_management",
   "version": "1.0.0",
   "type": "module",
   "@comments scripts": {
      "dev": "run all dev:* scripts",
      "dev:routify": "generate Routify's ro  utes.js on filechange",
      "build": "run all build:* scripts",
      "build:routify": "build routify",
      "serve": "serve content in 'dist' folder"
   },
   "scripts": {
      "dev": "run-p dev:*",
      "dev:routify": "routify",
      "preview": "vite preview",
      "build": "cross-env NODE_ENV=production run-s build:*",
      "build:routify": "routify -b",
      "test": "npm run test:dev && npm run test:build",
      "test:dev": "node test/lib/wrapper dev ava test/{common,dev}/**/*.test.js",
      "test:build": "node test/lib/wrapper build ava test/{common,build}/**/*.test.js",
      "dev:vite": "vite --host",
      "build:vite": "vite build",
      "validate": "svelte-check"
   },
   "devDependencies": {
      "@rollup/plugin-typescript": "11.1.2",
      "@roxi/routify": "^2.18.12",
      "@sveltejs/vite-plugin-svelte": "^2.4.2",
      "@tsconfig/svelte": "^5.0.0",
      "@zerodevx/svelte-toast": "^0.9.5",
      "autoprefixer": "^10.4.14",
      "carbon-icons-svelte": "^12.1.0",
      "cross-env": "^7.0.3",
      "cssnano": "^6.0.1",
      "fkill": "^8.1.1",
      "npm-run-all": "^4.1.5",
      "postcss": "^8.4.27",
      "postcss-import": "^15.1.0",
      "svelte": "^4.0.5",
      "svelte-check": "^3.4.6",
      "svelte-feather-icons": "^4.0.1",
      "svelte-hmr": "^0.15.3",
      "svelte-preprocess": "^5.0.4",
      "svelte-select": "^5.6.1",
      "tailwindcss": "^3.3.3",
      "tslib": "^2.6.1",
      "typescript": "^5.0.2",
      "vite": "^4.4.5"
   },
   "routify": {
      "extensions": "svelte,html,svx,md"
   },
   "appConfig": {
      "port": 5000,
      "assetsDir": "public",
      "template": "dist/index.html",
      "distDir": "dist",
      "buildDir": "dist/assets",
      "script": "dist/main.js"
   },
   "ava": {
      "verbose": true
   },
   "workspaces": [
      "../../*"
   ],
   "dependencies": {
      "axios": "^1.4.0",
      "moment": "^2.29.4",
      "svelte-forms-lib": "^2.0.1",
      "svelte-gantt": "^4.0.9-beta",
      "svelte-local-storage-store": "^0.5.0",
      "svelte-simple-modal": "^1.6.1"
   }
}
